docker build . -t html
docker images
docker run html
docker ps
// zie hij draait
docker stop html
docker ps
// lege lijst
docker run --name html_instance -d -p 80:80 html
// zie dat het nu is gestart
// kan je nu stoppen
// kan niet nogmaals starten
docker stop html_instance
docker run --name html_instance -d -p 80:80 html
// KRAK
// eerst removen
docker rm html_instance
// nu kan het weer worden opgestart

